<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" href="/docs/4.0/assets/img/favicons/favicon.ico">

    <title>TESTING GIAI TOAN</title>

    <!-- Bootstrap core CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/css/bootstrap.min.css"
        integrity="sha384-B0vP5xmATw1+K9KRQjQERJvTumQW0nPEzvF6L/Z6nronJ3oUOFUFpCjEUQouq2+l" crossorigin="anonymous">
    <!-- Custom styles for this template -->
    <link href="/css/style.css" rel="stylesheet">
</head>

<body>

    <!-- MENU -->
    <%-include('./menu.ejs')%>
    <!-- Begin page content -->
    <main role="main" class="container mt-2">
        <!-- DE BAI -->
        <div class="form-group content mx-auto mb-2">
            <h1 class="text-white text-center" id="label-de-bai">Giải toán nhanh</h1>
            <!-- DE BAI -->
            <div class="bg-white p-5">
                <form method="POST">
                    <div class="form-group">
                        <label for="formGroupExampleInput">Không gian mẫu</label>
                        <!-- INPUT -->
                        <input type="text" class="form-control" id="lk-A" placeholder="Nhập không gian mẫu">
                        
                        <div class="form-row d-none" id='dac-ta-A'>
                            <div class="form-group col-md-4">
                                <label for="inputCity"><small>Biểu thức trái</small></label>
                                <input type="text" disabled class="form-control" id="input-left-expr-A">
                            </div>
                            <div class="form-group col-md-4">
                                <label for="inputState"><small>Quan hệ đại số</small></label>
                                <select id="inputState" class="form-control">
                                    <option selected>Tập số nguyên</option>
                                </select>
                            </div>
                            <div class="form-group col-md-4">
                                <label for="inputZip"><small>Biểu thức phải</small></label>
                                <input type="text" disabled class="form-control" id="input-right-expr-A">
                            </div>
                        </div>
                        
                        <!-- END INPUT -->
                        <div class="mt-2">
                            <div class="form-check d-inline-block mr-3">
                                <input class="form-check-input" type="radio" name="kind-A" id="kind-A-1" value="option1" checked>
                                <label class="form-check-label" for="kind-A-1">
                                    <small>Liệt kê</small>
                                </label>
                            </div>
                            <div class="form-check d-inline-block ">
                                <input class="form-check-input" type="radio" name="kind-A" id="kind-A-2" value="option2">
                                <label class="form-check-label" for="kind-A-2">
                                    <small>Đặc tả</small>
                                </label>
                            </div>
                        </div>
                    </div>
                    <!-- ROW -->
                    <div class="form-group mt-4">
                        <label for="formGroupExampleInput2">Quan hệ</label>
                        <!-- INPUT -->
                        <input type="text" class="form-control" id="lk-R" placeholder="Nhập quan hệ">
                        <div class="form-row d-none " id='dac-ta-R'>
                            <div class="form-group col-md-4">
                              <label for="inputCity"><small>Biểu thức trái</small></label>
                              <input type="text" class="form-control" id="input-left-expr-R">
                            </div>
                            <div class="form-group col-md-4">
                              <label for="inputState"><small>Quan hệ đại số</small></label>
                              <select id="dk-R" class="form-control">
                                <option value="0" selected>=</option>
                                <option value="1">></option>
                                <option value="2">>=</option>
                                <option value="3"><</option>
                                <option value="4">=<</option>
                                <option value="5">Chia hết</option>
                                <option value="6">Ước số</option>
                                <option value="7">Bội số</option>
                                <option value="8">Là số chẵn</option>
                                <option value="9">Là số lẻ</option>
                              </select>
                            </div>
                            <div class="form-group col-md-4">
                              <label for="inputZip"><small>Biểu thức phải</small></label>
                              <input type="text" class="form-control" id="input-right-expr-R">
                            </div>
                        </div>
                        <!-- END INPUT -->
                        <div class="mt-2">
                            <div class="form-check d-inline-block mr-3">
                                <input class="form-check-input" type="radio" name="kind-R" id="kind-R-1"
                                    value="option1" checked>
                                <label class="form-check-label" for="kind-R-1">
                                    <small>Liệt kê</small> 
                                </label>
                            </div>
                            <div class="form-check d-inline-block ">
                                <input class="form-check-input" type="radio" name="kind-R" id="kind-R-2"
                                    value="option2">
                                <label class="form-check-label" for="kind-R-2">
                                    <small>Đặc tả</small> 
                                </label>
                            </div>
                        </div>
                    </div>
                </form>

                <!-- BUTTON CONTROL -->
                <div class='foot mt-3 text-center border-bottom pb-3'>
                    <a href="<%=link%>"  id='nop' class="btn btn-success font-itim">Nộp</a>
                    <button type="button" class="btn btn-danger font-itim ">Xóa</button>
                </div>
                <!-- END BUTTON CONTROL -->
            </div>
            <!-- END DE BAI -->
        </div>
        <!-- LOI GIAI BAI TAP -->
        <div class="bai-giai d-none ">
            <h3 class="text-white text-center" id="label-de-bai">Giải:</h3>
            <!-- <div class="tien-de-giai">
                Ta tiến hành tách hai vế của mệnh đề được như sau:
                <div id='ve-trai'>- <b>VT </b>:</div>
                <div id='ve-phai'>- <b>VP </b>:</div>
                Ta tiến hành chứng minh VT tương đương VP như sau:
            </div> -->
            <div class="bg-white p-5" id='chi-tiet-loi-giai'></div>
        </div>
    </main>
    <!-- END CONTENT -->

    <!-- FOOTER -->
    <footer class="footer">
        <div class="container  text-center">
            <span class="text-white font-italic">Website đang được xây dựng bởi Khonng Nho Ten</span>
        </div>
    </footer>
    <!-- END FOOTER -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"
        integrity="sha512-894YE6QWD5I59HgZOGReFYm4dnWc1Qt5NtvYSaNcOP+u1T9qYdvdihz0PPSiiqn/+/3e7Jo4EaG7TubfWGUrMQ=="
        crossorigin="anonymous"></script>


        <script src="/js/DrawHasse.js"></script>
        <script src="/js/Menu.js"></script>
    <script src="/js/quanhe2ngoi.js"></script>
</body>

</html>